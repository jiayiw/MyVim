syntax on
set number
set ts=4
set smartindent
set shiftwidth=4
set expandtab
set softtabstop=4
syntax enable
set background=dark
let g:solarized_termcolors = 256
set cursorline
set cursorcolumn
set tabstop=4
"设置自动缩进
set ai!
""缩进的字符个数
set cindent shiftwidth=4
set ruler
"colorscheme murphy
"colorscheme evening
let &termencoding=&encoding
set termencoding=utf-8
set fileencodings=utf-8,gbk
"let Tlist_Ctags_Cmd='/usr/local/bin/ctags'
set autochdir
set csprg=/usr/local/bin/cscope
set cst
"use cscope as the ctag
set cscopetag
